{
    "rust-analyzer.updates.askBeforeDownload": false,
    "rust-analyzer.cargo-watch.enable": true,
    "rust-analyzer.checkOnSave.allTargets": true,
    "rust-analyzer.checkOnSave.enable": true,
    "rust-analyzer.diagnostics.enable": true,
    "rust-analyzer.callInfo.full": true,
    "rust-analyzer.completion.addCallParenthesis": true,
    "rust-analyzer.completion.addCallArgumentSnippets": true,
    "rust-analyzer.inlayHints.typeHints": true,
    "rust-analyzer.inlayHints.parameterHints": true,
    "rust-analyzer.inlayHints.chainingHints": true,
    "rust-analyzer.inlayHints.maxLength": 50,
    "rust-analyzer.cargo.loadOutDirsFromCheck": true,
    "rust-analyzer.procMacro.enabled": true,
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.updates.channel": "stable",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "target": true,
        ".history": true,
        "Cargo.lock": true,
    }
}